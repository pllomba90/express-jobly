{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Software Engineer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100000
            ]
        },
        "equity": {
            "type": "boolean",
            "default": false,
            "title": "The equity Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "title": "Software Engineer",
        "salary": 100000,
        "equity": true
    }]
}